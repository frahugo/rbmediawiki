# -*- ruby -*-

require 'rubygems'
require 'hoe'
require './lib/rbmediawiki.rb'

Hoe.new('rbmediawiki', Rbmediawiki::VERSION) do |p|
   p.rubyforge_name = 'rbmediawiki' # if different than lowercase project name
   p.developer('Chabacano', 'Chabacano@chabawiki.com')
   p.remote_rdoc_dir = 'rdoc' #Release to root
end

